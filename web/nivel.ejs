<header>
    <%- include('header')-%>
</header>
<br/>
<h1>NIVELES <%=modelo.length%>
</h1>

<div class="container mt-5">
    <div class="row">
        <h5>ENG</h5>
    </div>
    <div class="col-md-3">
        <select name="humedad">
                <%if(modelo){%>
                <%for (var i=0; i<modelo.length; i++) {%>
                    <option> <%=modelo[i].ENG %></option>
                    <%}%>
                <%}%>
            
        </select>
    </div>
</div>

<div class="container" >
    <div class=" row mt-5">
    <div class="col-md-3">
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>ENG</th>
            </tr>
        </thead>

        <tbody id="tbody">
            <% for (const nivel of modelo) { %>
                <tr>
                    <td>
                        <%=nivel.id%>
                    </td>
                    <td>
                        <%=nivel.ENG%>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>
</div>
</header>